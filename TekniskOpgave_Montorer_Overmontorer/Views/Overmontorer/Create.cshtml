@model TekniskOpgave.DAL.Models.Overmontor

@{
    ViewData["Title"] = "Opret Overmontør";
}

<h1>Opret Overmontør</h1>

<form method="post">
    <div class="form-group">
        <label for="OvermontorNavn">Navn</label>
        <input type="text" class="form-control" id="OvermontorNavn" name="OvermontorNavn" required />
    </div>
    <div class="form-group">
        <label for="OvermontorTelefonnummer">Telefonnummer</label>
        <input type="text" class="form-control" id="OvermontorTelefonnummer" name="OvermontorTelefonnummer" required />
    </div>
    <button type="submit" class="btn btn-primary">Opret</button>
</form>

<div>
    <a href="@Url.Action("Index", "Overmontorer")">Back to List</a>
</div>
